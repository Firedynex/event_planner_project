<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="20.0"
  xmlns="http://javafx.com/javafx/8.0.171" fx:controller="event_planner.SecondaryController">
  <children>
    <Button onAction="#switchToPrimary" text="Switch to Primary View" fx:id="secondaryButton"/>
    <Label text="Secondary View"/>
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>
</VBox>
